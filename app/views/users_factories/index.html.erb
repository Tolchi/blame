<h1>Listing users_factories</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Pass</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users_factories.each do |users_factory| %>
  <tr>
    <td><%= users_factory.email %></td>
    <td><%= users_factory.pass %></td>
    <td><%= link_to 'Show', users_factory %></td>
    <td><%= link_to 'Edit', edit_users_factory_path(users_factory) %></td>
    <td><%= link_to 'Destroy', users_factory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

